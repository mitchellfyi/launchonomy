{
  "agents": {
    "OrchestrationAgent": {
      "endpoint": "orchestrator_agent.OrchestrationAgent",
      "certified": true,
      "spec": {}
    },
    "AutoProvisionAgent": {
      "module": "orchestrator.agents.auto_provision_agent",
      "class": "AutoProvisionAgent",
      "certified": true,
      "spec": {}
    },
    "ScanAgent": {
      "module": "orchestrator.agents.scan_agent",
      "class": "ScanAgent",
      "certified": true,
      "spec": {
        "description": "Identifies, researches, and ranks potential business opportunities based on market demand and competition analysis",
        "workflow_step": "scan_opportunities",
        "input_schema": {
          "mission_context": "dict",
          "focus_areas": "list[str]",
          "max_opportunities": "int"
        },
        "output_schema": {
          "opportunities": "list[dict]",
          "recommendations": "dict",
          "scan_metadata": "dict"
        },
        "type": "workflow_agent"
      }
    },
    "DeployAgent": {
      "module": "orchestrator.agents.deploy_agent",
      "class": "DeployAgent",
      "certified": true,
      "spec": {
        "description": "Builds and deploys minimum viable products (MVPs) based on selected opportunities",
        "workflow_step": "deploy_mvp",
        "input_schema": {
          "opportunity": "dict",
          "requirements": "dict",
          "budget_limit": "float"
        },
        "output_schema": {
          "product_details": "dict",
          "deployment_summary": "dict",
          "cost_breakdown": "dict"
        },
        "type": "workflow_agent"
      }
    },
    "CampaignAgent": {
      "module": "orchestrator.agents.campaign_agent",
      "class": "CampaignAgent",
      "certified": true,
      "spec": {
        "description": "Manages customer acquisition campaigns and continuously optimizes them for better conversion rates",
        "workflow_step": "run_campaigns",
        "input_schema": {
          "campaign_type": "str",
          "product_details": "dict",
          "budget_allocation": "dict"
        },
        "output_schema": {
          "execution_results": "dict",
          "performance": "dict",
          "budget_utilization": "dict"
        },
        "type": "workflow_agent"
      }
    },
    "AnalyticsAgent": {
      "module": "orchestrator.agents.analytics_agent",
      "class": "AnalyticsAgent",
      "certified": true,
      "spec": {
        "description": "Collects, analyzes, and reports on key business metrics including revenue, customer acquisition, and conversion rates",
        "workflow_step": "fetch_metrics",
        "input_schema": {
          "analysis_type": "str",
          "time_period": "str",
          "specific_metrics": "list[str]"
        },
        "output_schema": {
          "processed_metrics": "dict",
          "insights": "dict",
          "alerts": "list[dict]"
        },
        "type": "workflow_agent"
      }
    },
    "FinanceAgent": {
      "module": "orchestrator.agents.finance_agent",
      "class": "FinanceAgent",
      "certified": true,
      "spec": {
        "description": "Handles financial guardrails and budget enforcement to ensure costs never exceed 20% of revenue",
        "workflow_step": "enforce_guardrail",
        "input_schema": {
          "operation_type": "str",
          "estimated_cost": "float",
          "time_period": "str"
        },
        "output_schema": {
          "approval_status": "str",
          "budget_status": "dict",
          "risk_assessment": "dict"
        },
        "type": "workflow_agent"
      }
    },
    "GrowthAgent": {
      "module": "orchestrator.agents.growth_agent",
      "class": "GrowthAgent",
      "certified": true,
      "spec": {
        "description": "Handles growth loop execution and optimization for sustainable, profitable growth",
        "workflow_step": "run_growth_loop",
        "input_schema": {
          "growth_phase": "str",
          "current_metrics": "dict",
          "experiment_budget": "float"
        },
        "output_schema": {
          "execution_results": "dict",
          "optimization_results": "dict",
          "growth_score": "float"
        },
        "type": "workflow_agent"
      }
    }
  },
